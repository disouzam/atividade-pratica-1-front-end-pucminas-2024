{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": [
            "error",
             2
        ],
        "max-len": [
            "error", 
            { 
                "code": 100,
                "ignoreTrailingComments": true,
                "ignoreComments": true
            }
        ]
    }
}
